{
  "name": "zomoc",
  "version": "1.0.0",
  "description": "An automatic API mocking tool for Vite, powered by axios and Zod.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "zomoc": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "axios": ">=0.21.0",
    "vite": ">=5.0.0",
    "zod": ">=3.0.0"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "commander": "^12.0.0",
    "glob": "^10.4.1",
    "micromatch": "^4.0.7",
    "path-to-regexp": "^8.2.0",
    "ts-to-zod": "^3.1.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/micromatch": "^4.0.7",
    "@types/node": "^20.12.12",
    "@types/path-to-regexp": "^1.7.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "axios",
    "mock",
    "interceptor",
    "zod",
    "typescript",
    "vite",
    "plugin"
  ],
  "author": "",
  "license": "MIT"
}
